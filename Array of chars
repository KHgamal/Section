using System;
  
public class charArray{
    
     public static void Main(string[] args)
    { 
        string str = "open source software"; 
    
       
        char[] ch = new char[str.Length]; 
    
    
        for (int i = 0; i < str.Length; i++) { 
            ch[i] = str[i]; 
        } 
  
    } 
}
